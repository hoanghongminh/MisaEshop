ACA,sBAAA,MAAA,CAAA,IAAA,CAAA,CAAA,KAAA,CAAA,MAAA,GAAA,EAAA;AACA,sBAAA,CAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA,CAAA,IAAA,CAAA,UAAA,EAAA,KAAA;AACA,qBAHA;AAIA,mBAZA,MAYA;AACA,oBAAA,KAAA,CAAA,gBAAA,CAAA,IAAA,EAAA,iBAAA,EAAA,IAAA;AACA;AACA,iBAlBA,EAmBA,KAnBA,CAmBA,YAAA;AACA,kBAAA,KAAA,CAAA,gBAAA,CACA,KADA,EAEA,mCAFA,EAGA,KAHA;AAKA,iBAzBA,CA/CA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0EA,KAhGA;AAkGA;AACA;AACA,IAAA,gBApGA,4BAoGA,IApGA,EAoGA,GApGA,EAoGA,MApGA,EAoGA;AACA,UAAA,IAAA,IAAA,IAAA,EAAA;AACA;AACA,aAAA,QAAA;AACA,OAJA,CAKA;;;AACA,WAAA,KAAA,CAAA,UAAA;AACA,WAAA,YAAA,CAAA,GAAA,GAAA,GAAA;AACA,WAAA,YAAA,CAAA,MAAA,GAAA,MAAA;AACA,WAAA,KAAA,CAAA,gBAAA,EAAA,KAAA,YAAA;AACA,WAAA,KAAA,CAAA,cAAA,EAAA,KAAA;AACA,KA/GA;AAiHA;AACA;AACA,IAAA,UAnHA,sBAmHA,MAnHA,EAmHA;AACA,UAAA,KAAA,GAAA,CAAA,CAAA,MAAA,MAAA,CAAA,CAAA,GAAA,EAAA;;AACA,UAAA,CAAA,KAAA,EAAA;AACA,QAAA,CAAA,CAAA,MAAA,MAAA,CAAA,CAAA,QAAA,CAAA,YAAA;AACA,QAAA,CAAA,CAAA,MAAA,MAAA,GAAA,QAAA,CAAA,CAAA,GAAA,CAAA,SAAA,EAAA,OAAA;AACA,QAAA,CAAA,CAAA,MAAA,MAAA,CAAA,CAAA,IAAA,CAAA,UAAA,EAAA,KAAA;AACA,OAJA,MAIA;AACA,QAAA,CAAA,CAAA,MAAA,MAAA,CAAA,CAAA,WAAA,CAAA,YAAA;AACA,QAAA,CAAA,CAAA,MAAA,MAAA,GAAA,QAAA,CAAA,CAAA,GAAA,CAAA,SAAA,EAAA,MAAA;AACA,QAAA,CAAA,CAAA,MAAA,MAAA,CAAA,CAAA,IAAA,CAAA,UAAA,EAAA,IAAA;AACA;AACA,KA9HA;AAgIA;AACA;AACA;AACA,IAAA,WAnIA,yBAmIA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,GAAA,CAAA,wCAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA;AACA,iBAFA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KAvIA;AAwIA;AACA;AACA,IAAA,YA1IA,0BA0IA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,GAAA,CAAA,yCAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;AACA,iBAFA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KA9IA;AA+IA;AACA;AACA,IAAA,YAjJA,0BAiJA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,GAAA,CAAA,yCAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA;AACA,iBAFA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KArJA;AAsJA;AACA;AACA,IAAA,QAxJA,sBAwJA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KAAA,CAAA,GAAA,CAAA,qCAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,kBAAA,MAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA;AACA,iBAFA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA;AA5JA,GAxBA;AAsLA,EAAA,YAtLA,0BAsLA,CAAA,CAtLA;AAuLA,EAAA,OAvLA,qBAuLA;AACA,SAAA,WAAA;AACA,SAAA,YAAA;AACA,SAAA,YAAA;AACA,SAAA,QAAA;AACA,GA5LA;AA6LA,EAAA,QAAA,EAAA;AACA;AACA;AACA,IAAA,oBAHA,kCAGA;AAAA;;AACA,aAAA,KAAA,SAAA,CAAA,MAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,SAAA,IAAA,MAAA,CAAA,KAAA,CAAA,SAAA;AAAA,OAAA,CAAA;AACA,KALA;AAMA;AACA;AACA,IAAA,qBARA,mCAQA;AAAA;;AACA,aAAA,KAAA,SAAA,CAAA,MAAA,CACA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,UAAA,IAAA,MAAA,CAAA,KAAA,CAAA,UAAA;AAAA,OADA,CAAA;AAGA,KAZA;AAaA;AACA;AACA,IAAA,iBAfA,+BAeA;AAAA;;AACA,aAAA,KAAA,KAAA,CAAA,MAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,UAAA,IAAA,MAAA,CAAA,KAAA,CAAA,UAAA;AAAA,OAAA,CAAA;AACA;AAjBA;AA7LA,CAAA","sourcesContent":["<template>\r\n  <!--dialog-->\r\n  <div class=\"m-dialog\" :class=\"{ isHideDialog: isHideDialog }\">\r\n    <div class=\"dialog-modal\"></div>\r\n    <div class=\"dialog dialog-content\">\r\n      <div class=\"form-header\">\r\n        <strong class=\"title item-center\">{{ this.isNameDialog }}</strong>\r\n        <div class=\"btn-x item-center\" @click=\"btnClose\">\r\n          <i class=\"icon-x\" title=\"Đóng\"></i>\r\n        </div>\r\n        <div class=\"clear\"></div>\r\n      </div>\r\n      <div class=\"form-body\">\r\n        <div class=\"content-center\">\r\n          <div class=\"input-requied\">\r\n            <div class=\"input-title\">\r\n              Mã cửa hàng\r\n              <span class=\"label-requied\">*</span>\r\n            </div>\r\n            <input\r\n              tabindex=\"1\"\r\n              id=\"storeCode\"\r\n              @blur=\"isValidate('storeCode')\"\r\n              type=\"text\"\r\n              name=\"\"\r\n              placeholder=\"\"\r\n              validate=\"false\"\r\n              ref=\"storeCode\"\r\n              required\r\n              v-model=\"store.storeCode\"\r\n              class=\"border-input-requied\"\r\n            />\r\n            <div>\r\n              <div class=\"talk-bubble\" id=\"storeCode_wrang\">\r\n                <div class=\"talktext\">\r\n                  <p>Dữ liệu không hợp lệ</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"input-requied\">\r\n            <div class=\"input-title\">\r\n              Tên cửa hàng\r\n              <span class=\"label-requied\">*</span>\r\n            </div>\r\n            <input\r\n              tabindex=\"2\"\r\n              id=\"storeName\"\r\n              validate=\"false\"\r\n              @blur=\"isValidate('storeName')\"\r\n              type=\"text\"\r\n              class=\"form-control border-input-requied\"\r\n              name=\"\"\r\n              placeholder=\"\"\r\n              ref=\"storeName\"\r\n              required\r\n              v-model=\"store.storeName\"\r\n            />\r\n            <div>\r\n              <div class=\"talk-bubble\" id=\"storeName_wrang\">\r\n                <div class=\"talktext\">\r\n                  <p>Dữ liệu không hợp lệ</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"input-requied\">\r\n            <div class=\"input-title\">\r\n              Địa chỉ\r\n              <span class=\"label-requied\">*</span>\r\n            </div>\r\n            <textarea\r\n              tabindex=\"3\"\r\n              @blur=\"isValidate('storeAddress')\"\r\n              id=\"storeAddress\"\r\n              ref=\"address\"\r\n              validate=\"false\"\r\n              v-model=\"store.address\"\r\n              required\r\n              class=\"border-input-requied\"\r\n            ></textarea>\r\n            <div>\r\n              <div class=\"talk-bubble\" id=\"storeAddress_wrang\">\r\n                <div class=\"talktext\">\r\n                  <p>Dữ liệu không hợp lệ</p>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n          <div class=\"store-left\">\r\n            <label class=\"input-title\">Số điện thoại</label>\r\n            <input\r\n              tabindex=\"4\"\r\n              type=\"text\"\r\n              class=\"input-notrequied border-input-requied\"\r\n              name=\"\"\r\n              placeholder=\"\"\r\n              v-model=\"store.phoneNumber\"\r\n            />\r\n          </div>\r\n          <div class=\"store-right\">\r\n            <label class=\"input-title\">Mã số thuế</label>\r\n            <input\r\n              tabindex=\"5\"\r\n              type=\"text\"\r\n              class=\"input-notrequied border-input-requied\"\r\n              v-model=\"store.storeTaxCode\"\r\n            />\r\n          </div>\r\n          <div class=\"clear\"></div>\r\n          <div class=\"store-left\">\r\n            <label class=\"input-title\">Quốc gia</label>\r\n            <select\r\n              tabindex=\"6\"\r\n              class=\"input-notrequied border-input-requied\"\r\n              v-model=\"store.countryId\"\r\n            >\r\n              <option\r\n                v-for=\"country in this.countrys\"\r\n                :key=\"country.countryId\"\r\n                :value=\"country.countryId\"\r\n              >\r\n                {{ country.countryName }}\r\n              </option>\r\n            </select>\r\n          </div>\r\n          <div class=\"clear\"></div>\r\n          <div class=\"store-left\">\r\n            <label class=\"input-title\">Tỉnh/Thành phố</label>\r\n            <select\r\n              tabindex=\"7\"\r\n              class=\"input-notrequied border-input-requied\"\r\n              v-model=\"store.provinceId\"\r\n            >\r\n              <option\r\n                v-for=\"province in getProvinceByCountry\"\r\n                :key=\"province.provinceId\"\r\n                :value=\"province.provinceId\"\r\n              >\r\n                {{ province.provinceName }}\r\n              </option>\r\n            </select>\r\n          </div>\r\n          <div class=\"store-right\">\r\n            <label class=\"input-title\">Quận/Huyện</label>\r\n            <select\r\n              tabindex=\"8\"\r\n              class=\"input-notrequied border-input-requied\"\r\n              v-model=\"store.districtId\"\r\n            >\r\n              <option\r\n                v-for=\"district in getDistrictByProvince\"\r\n                :key=\"district.districtId\"\r\n                :value=\"district.districtId\"\r\n              >\r\n                {{ district.districtName }}\r\n              </option>\r\n            </select>\r\n          </div>\r\n          <div class=\"clear\"></div>\r\n          <div class=\"store-left\">\r\n            <label class=\"input-title\">Phường/Xã</label>\r\n            <select\r\n              tabindex=\"9\"\r\n              class=\"input-notrequied border-input-requied\"\r\n              v-model=\"store.wardId\"\r\n            >\r\n              <option\r\n                v-for=\"ward in getWardByDistrict\"\r\n                :key=\"ward.wardId\"\r\n                :value=\"ward.wardId\"\r\n              >\r\n                {{ ward.wardName }}\r\n              </option>\r\n            </select>\r\n          </div>\r\n          <div class=\"store-right\">\r\n            <label class=\"input-title\">Đường phố</label>\r\n            <input\r\n              tabindex=\"10\"\r\n              type=\"text\"\r\n              class=\"input-notrequied border-input-requied\"\r\n              v-model=\"store.street\"\r\n            />\r\n          </div>\r\n          <div class=\"clear\"></div>\r\n        </div>\r\n      </div>\r\n      <div class=\"form-footer\">\r\n        <div class=\"store-left-footer\">\r\n          <i class=\"icon-help\"></i>\r\n          <strong>Trợ giúp</strong>\r\n        </div>\r\n        <div class=\"store-right-footer\">\r\n          <button class=\"btn-save item-center\" @click=\"saveStore\" tabindex=\"11\">\r\n            <i class=\"icon-save\"></i>\r\n            <p style=\"color: white\">Lưu</p>\r\n          </button>\r\n          <button class=\"btn-add item-center\" tabindex=\"12\" @click=\"btnSaveAdd\">\r\n            <i class=\"icon-add\"></i>\r\n            <p class=\"text-insert\">Lưu và thêm mới</p>\r\n          </button>\r\n          <div class=\"btn-cancel item-center\" @click=\"btnClose\" tabindex=\"13\">\r\n            <i class=\"icon-x\"></i>\r\n            <p class=\"text-cancel\">Hủy bỏ</p>\r\n          </div>\r\n        </div>\r\n        <div class=\"clear\"></div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport $ from \"jquery\";\r\nimport axios from \"axios\";\r\nexport default {\r\n  name: \"StoreListDetail\",\r\n  data() {\r\n    return {\r\n      // ĐỐi tượng hiển thị trong combobox\r\n      // CreatedBy: \r\n      countrys: [],\r\n      provinces: [],\r\n      districts: [],\r\n      wards: [],\r\n      // Đối tượng trả về cho Snackbar\r\n      // CreatedBy: \r\n      dataSnackBar: {\r\n        msg: \"\",\r\n        status: true,\r\n      },\r\n      // Biến chức năng\r\n      // CreatedBy: n\r\n      isSaveOrAdd: false,\r\n      isHideToolTip: true,\r\n      isAddOrUpdate: true,\r\n    };\r\n  },\r\n  props: [\"isHideDialog\", \"store\", \"formMode\", \"isNameDialog\"],\r\n  methods: {\r\n    //Sự kiện kích vào nút lưu và thêm mới\r\n    // CreatedBy: \r\n    btnSaveAdd() {\r\n      this.isSaveOrAdd = true;\r\n      this.saveStore();\r\n    },\r\n\r\n    //Sự kiện hủy trong Dialog\r\n    // CreatedBy: \r\n    btnClose() {\r\n      $(\"#storeCode_wrang\").css(\"display\", \"none\");\r\n      $(\"#storeName_wrang\").css(\"display\", \"none\");\r\n      $(\"#storeAddress_wrang\").css(\"display\", \"none\");\r\n      $(\"#storeCode\").removeClass(\"border-red\");\r\n      $(\"#storeName\").removeClass(\"border-red\");\r\n      $(\"#storeAddress\").removeClass(\"border-red\");\r\n      this.$emit(\"closeDialog\", true);\r\n    },\r\n\r\n    //Sự kiện nhấn vào nút Lưu/Sửa trong Dialog\r\n    // CreatedBy: \r\n    async saveStore() {\r\n      const me = this;\r\n\r\n      //Validate dữ liệu\r\n      var inputValidates = $(\"input[required], textarea[required]\");\r\n      $.each(inputValidates, function (index, input) {\r\n        var entity = inputValidates[index].id;\r\n        var value = $(\"#\" + entity).val();\r\n        // Thêm border và hiện tooltip cho input lỗi\r\n        if (!value) {\r\n          $(\"#\" + entity).addClass(\"border-red\");\r\n          $(\"#\" + entity + \"_wrang\").css(\"display\", \"block\");\r\n        } else {\r\n          $(\"#\" + entity).removeClass(\"border-red\");\r\n          $(\"#\" + entity + \"_wrang\").css(\"display\", \"none\");\r\n        }\r\n        $(input).trigger(\"blur\");\r\n      });\r\n\r\n      // //Focus vào input đầu tiên nếu có lỗi\r\n      // CreatedBy: \r\n      var inputNotValidate = $(\r\n        'input[validate=\"false\"], textarea[validate=\"false\"]'\r\n      );\r\n      if (inputNotValidate && inputNotValidate.length > 0) {\r\n        inputNotValidate[0].focus();\r\n        return;\r\n      }\r\n\r\n      //Kiểm tra nếu trạng thái là sửa\r\n      // CreatedBy: \r\n      if (this.formMode == 0) {\r\n        await axios\r\n          .put(\"http://localhost:52139/api/v1/stores\", me.store)\r\n          .then(() => {\r\n            this.handlingSnackbar(true, \"Sửa thành công\", true);\r\n          })\r\n          .catch(() => {\r\n            // Lấy ra thông báo lỗi\r\n            this.handlingSnackbar(\r\n              false,\r\n              \"Mã khách hàng đã có trên hệ thống\",\r\n              false\r\n            );\r\n          });\r\n      } else {\r\n        //Ngược lại thì sẽ là trạng thái thêm\r\n        await axios\r\n          .post(\"http://localhost:52139/api/v1/stores\", me.store)\r\n          .then(() => {\r\n            if (this.isSaveOrAdd == true) {\r\n              // Gọi hàm thay đổi đối tượng store về giỗng\r\n              this.$emit(\"changStore\", true);\r\n              this.handlingSnackbar(false, \"Thêm thành công\", true);\r\n              this.$refs.storeCode.focus();\r\n              // gán lại border cho input và đặt lại validate = false\r\n              var inputs = this.$refs;\r\n              var array = Object.keys(inputs);\r\n              array.map((item) => {\r\n                inputs[item].style.border = \"\";\r\n                $(inputs[item]).attr(\"validate\", false);\r\n              });\r\n            } else {\r\n              this.handlingSnackbar(true, \"Thêm thành công\", true);\r\n            }\r\n          })\r\n          .catch(() => {\r\n            this.handlingSnackbar(\r\n              false,\r\n              \"Mã khách hàng đã có trên hệ thống\",\r\n              false\r\n            );\r\n          });\r\n      }\r\n    },\r\n\r\n    // Xử lý Snackbar\r\n    // CreatedBy: \r\n    handlingSnackbar(bool, msg, status) {\r\n      if (bool == true) {\r\n        //Thàng công thì gọi hàm đóng Dialog\r\n        this.btnClose();\r\n      }\r\n      //Gọi hàm Load lại dữ liệu trong bảng\r\n      this.$emit(\"loadData\");\r\n      this.dataSnackBar.msg = msg;\r\n      this.dataSnackBar.status = status;\r\n      this.$emit(\"changeSnackBar\", this.dataSnackBar);\r\n      this.$emit(\"showSnackbar\", false);\r\n    },\r\n\r\n    //Hàm validate dữ liệu nhập vào khi blur\r\n    // CreatedBy:\r\n    isValidate(entity) {\r\n      var value = $(\"#\" + entity).val();\r\n      if (!value) {\r\n        $(\"#\" + entity).addClass(\"border-red\");\r\n        $(\"#\" + entity + \"_wrang\").css(\"display\", \"block\");\r\n        $(\"#\" + entity).attr(\"validate\", false);\r\n      } else {\r\n        $(\"#\" + entity).removeClass(\"border-red\");\r\n        $(\"#\" + entity + \"_wrang\").css(\"display\", \"none\");\r\n        $(\"#\" + entity).attr(\"validate\", true);\r\n      }\r\n    },\r\n\r\n    //Các hàm xử lý dữ liệu vào trong combobox\r\n    // Hàm load dữ liệu của quốc gia\r\n    // CreatedBy: \r\n    async loadCountry() {\r\n      await axios.get(\"http://localhost:52139/api/v1/countrys\").then((res) => {\r\n        this.countrys = res.data;\r\n      });\r\n    },\r\n    // Hàm load dữ liệu của tỉnh thành phố\r\n    // CreatedBy: \r\n    async loadProvince() {\r\n      await axios.get(\"http://localhost:52139/api/v1/provinces\").then((res) => {\r\n        this.provinces = res.data;\r\n      });\r\n    },\r\n    // Hàm load dữ liệu của tỉnh thành phố\r\n    // CreatedBy: \r\n    async loadDistrict() {\r\n      await axios.get(\"http://localhost:52139/api/v1/districts\").then((res) => {\r\n        this.districts = res.data;\r\n      });\r\n    },\r\n    // Hàm load dữ liệu của tỉnh thành phố\r\n    // CreatedBy: \r\n    async loadWard() {\r\n      await axios.get(\"http://localhost:52139/api/v1/wards\").then((res) => {\r\n        this.wards = res.data;\r\n      });\r\n    },\r\n  },\r\n  beforeUpdate() {},\r\n  created() {\r\n    this.loadCountry();\r\n    this.loadProvince();\r\n    this.loadDistrict();\r\n    this.loadWard();\r\n  },\r\n  computed: {\r\n    //Lấy tỉnh theo id quốc gia\r\n    // CreatedBy: \r\n    getProvinceByCountry() {\r\n      return this.provinces.filter((x) => x.countryId == this.store.countryId);\r\n    },\r\n    //Lấy quận huyện theo id thành phố\r\n    // CreatedBy: \r\n    getDistrictByProvince() {\r\n      return this.districts.filter(\r\n        (x) => x.provinceId == this.store.provinceId\r\n      );\r\n    },\r\n    //Lấy phường xã theo id quận huyện\r\n    // CreatedBy:\r\n    getWardByDistrict() {\r\n      return this.wards.filter((x) => x.districtId == this.store.districtId);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n/* css cho tooltip */\r\n.isHideToolTip {\r\n  display: none;\r\n}\r\n.red {\r\n  background-color: #f63233;\r\n  color: #ffffff;\r\n}\r\n.black {\r\n  background-color: #000000;\r\n  color: #ffffff;\r\n}\r\n.talk-bubble {\r\n  width: 200px;\r\n  height: 34px;\r\n  background-color: #f63233;\r\n  color: #fff;\r\n  text-align: center;\r\n  border-radius: 4px;\r\n  position: absolute;\r\n  z-index: 99999999999;\r\n  top: 5px;\r\n  left: 101%;\r\n  display: none;\r\n  color: #fbc5c8;\r\n}\r\n.talk-bubble:after {\r\n  content: \" \";\r\n  position: absolute;\r\n  width: 0;\r\n  height: 0;\r\n  left: -22px;\r\n  right: auto;\r\n  top: 1px;\r\n  bottom: auto;\r\n  border: 16px solid;\r\n  border-color: transparent #f63233 transparent transparent;\r\n}\r\n/* .talk-bubble-country:after{\r\n  left: -368px;\r\n  top: 268px;\r\n} */\r\n.talktext {\r\n  padding: 0.6em;\r\n}\r\n.component-fade-enter-active {\r\n  animation: bounce-in 0.5s;\r\n}\r\n.component-fade-leave-active {\r\n  animation: bounce-out 0.5s;\r\n}\r\n@keyframes bounce-in {\r\n  0% {\r\n    opacity: 0;\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n  }\r\n}\r\n@keyframes bounce-out {\r\n  100% {\r\n    opacity: 0;\r\n  }\r\n}\r\n.text-cancel{\r\n  margin-right: 6px;\r\n\r\n}\r\n</style>"],"sourceRoot":"src/views/dictionary/stores"}]}